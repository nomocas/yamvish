{
  "name": "yamvish",
  "version": "0.3.0",
  "description": "Yet Another MVish javascript library",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha",
    "build": "npm run build-core && npm run build-yamvish && gulp uglify",
    "build-yamvish": "browserify -s y index.js > dist/yamvish.js",
    "build-core": "browserify -s y core.js > dist/yamvish.core.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nomocas/yamvish.git"
  },
  "keywords": [
    "mvvm",
    "mvc",
    "js",
    "data-bind",
    "templating",
    "observable"
  ],
  "author": "Gilles Coomans <gilles.coomans@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nomocas/yamvish/issues"
  },
  "homepage": "https://github.com/nomocas/yamvish",
  "devDependencies": {
    "chai": "^2.2.0",
    "gulp": "^3.9.0",
    "gulp-browserify": "^0.5.1",
    "gulp-jshint": "^1.10.0",
    "gulp-live-server": "0.0.14",
    "gulp-rename": "^1.2.2",
    "gulp-uglifyjs": "^0.6.1",
    "jshint-stylish": "^1.0.1"
  },
  "dependencies": {
    "elenpi": "^0.2.5"
  }
}
