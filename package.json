{
  "name": "yamvish",
  "version": "0.7.2",
  "description": "Yet Another MVish javascript library",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha",
    "build:prod": "npm run build:core  && gulp uglify",
    "build:core": "browserify -s y index.js > dist/yamvish.js",
    "min": "uglifyjs dist/yamvish.js --compress --mangle  -o dist/yamvish.min.js",
    "serve": "live-server . --ignore=node_modules,lib",
    "watch": "watch 'browserify -s y index.js > dist/yamvish.js' ./lib",
    "watch:example": "watch 'browserify -s y example/main.js > example/bundle.js' ./lib",
    "bump:patch": "npm run build:prod && git add -A && git commit -m 'patch version' && git push && mversion patch -m && git push --tags && npm publish && git push",
    "bump:minor": "npm run build:prod && git add -A && git commit -m 'patch version' && git push && mversion minor -m && git push --tags && npm publish && git push",
    "bump:major": "npm run build:prod && git add -A && git commit -m 'patch version' && git push && mversion major -m && git push --tags && npm publish && git push"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nomocas/yamvish.git"
  },
  "keywords": [
    "mvvm",
    "mvc",
    "js",
    "data-bind",
    "templating",
    "observable"
  ],
  "author": "Gilles Coomans <gilles.coomans@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nomocas/yamvish/issues"
  },
  "homepage": "https://github.com/nomocas/yamvish",
  "devDependencies": {
    "chai": "^2.2.0",
    "gulp": "^3.9.0",
    "gulp-browserify": "^0.5.1",
    "gulp-jshint": "^1.10.0",
    "gulp-live-server": "0.0.14",
    "gulp-rename": "^1.2.2",
    "gulp-uglifyjs": "^0.6.1",
    "jshint-stylish": "^1.0.1"
  },
  "dependencies": {
    "elenpi": "^0.2.5"
  }
}
